for i in range(6):
    for j in range(10):
        print("$", end = "")
    print("")
    
for i in range(1):
    for j in range(5):
        print("3", end = "")
    print("")
    
for i in range(1):
    for j in range(5):
        print("4", end = "")
    print("")

for i in range(1):
    for j in range(5):
        print("5", end = "")
    print("")
